
<h1>Create a new Wiki page</h1>

<%= form_for @page, url: wiki_page_index_url do |form| %>
  <%= form.fields_for :revisions, Revision.new do |ff| %>
  <div>
    <%= ff.label :title %>
    <%= ff.text_field :title %>

    <%= ff.label :content %>
    <%= ff.text_field :content %>
  </div>


<% end %>

  <button class="waves-effect waves-light btn orange accent-3" type="submit" name="action">Create Page
  </button>

<% end %>

<%= button_to 'Back', wiki_page_index_url, method: :get, class: "waves-effect waves-light btn blue accent-4"%>
